{
	"$schema": "../node_modules/@tauri-apps/cli/schema.json",
	"build": {
	  "beforeBuildCommand": "pnpm next start",
	  "beforeDevCommand": "pnpm next dev -p 3000",
	  "devPath": "http://localhost:3000",
	  "distDir": "../out"
	},
	"package": {
	  "productName": "index",
	  "version": "0.0.1"
	},
	"tauri": {
	  "allowlist": {
		"all": true
	  },
	  "bundle": {
		"active": true,
		"category": "DeveloperTool",
		"copyright": "",
		"deb": {
		  "depends": []
		},
		"externalBin": [],
		"icon": [
		  "icons/32x32.png",
		  "icons/128x128.png",
		  "icons/128x128@2x.png",
		  "icons/icon.icns",
		  "icons/icon.ico"
		],
		"identifier": "com.index",
		"longDescription": "",
		"macOS": {
		  "entitlements": null,
		  "exceptionDomain": "",
		  "frameworks": [],
		  "providerShortName": null,
		  "signingIdentity": null
		},
		"resources": [],
		"shortDescription": "",
		"targets": "all",
		"windows": {
		  "certificateThumbprint": null,
		  "digestAlgorithm": "sha256",
		  "timestampUrl": ""
		}
	  },
	  "security": {
		"csp": null
	  },
	  "updater": {
		"active": false
	  },
	  "windows": [
		{
		  "fullscreen": false,
		  "resizable": true,
		  "title": "index",
		  "width": 1280,
		  "height": 800
		}
	  ]
	}
  }